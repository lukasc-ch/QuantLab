{
  "architecture": {
    "class": "VGG",
    "params": {
      "capacity": 1,
      "quantAct": true, 
      "quantActSTENumLevels": 3, 
      "quantWeights": true,
      "weightInqBits": 2, 
      "weightInqSchedule": {
          "150": 0.5,
          "230": 0.75,
          "310": 0.875,
          "390": 0.9375,
          "470": 0.96875,
          "550": 1.0
      },
      "quant_scheme": {}
    }
  },
  "thermostat": {
    "class": "VGG",
    "params": {
      "noise_scheme": {},
      "bindings": []
    }
  },
  "loss_fn": {
      "class": "HingeLoss",
    "params": {"num_classes": 10}
  },
  "optimizer": {
    "class": "Adam",
    "params": {"lr": 1e-3}
  },
  "lr_scheduler": {
    "class": "HandScheduler",
    "params": {
      "schedule": {
        "140": 0.1,
        "151": 1.0,
        "220": 0.1,
        "231": 1.0,
        "300": 0.1,
        "311": 1.0,
        "380": 0.1,
        "391": 1.0,
        "460": 0.1,
        "471": 1.0,
        "540": 0.1,
        "551": 1.0,
        "600": 0.1,
        "650": 0.01
      }
    }
  },
  "max_epoch": 750,
  "data": {
    "preprocess": {"augment": true, "valid_fraction": 0.1},
    "batch_size_train": 256,
    "batch_size_valid": 64
  }
}
